{
  "name": "html-dsl-generator",
  "type": "module",
  "module": "src/index.ts",
  "eslintConfig": {
    "extends": [
      "chimeraant"
    ]
  },
  "scripts": {
    "generate": "./scripts/generate.ts",
    "lint": "eslint . --ext ts",
    "typecheck": "bun typecheck:bun && bun typecheck:es",
    "typecheck:bun": "tsc -p src/bun/tsconfig.json --noEmit",
    "typecheck:es": "tsc -p src/es/tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "bun-types": "^0.5.7",
    "eslint": "^8.30.0",
    "eslint-config-chimeraant": "^1.2.9",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "html-tag-names": "^2.0.1"
  }
}
